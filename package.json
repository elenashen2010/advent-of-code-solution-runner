{
  "name": "advent-of-code-solution-runner",
  "version": "2.3.3",
  "description": "Boilerplate for running code to solve Advent of Code coding challenges.",
  "scripts": {
    "start": "ts-node src/main.ts",
    "start:dev": "cross-env DEBUG=true ts-node-dev --respawn --watch input src/main.ts",
    "create": "ts-node src/create.ts"
  },
  "bin": {
    "aoc-run": "entry.js"
  },
  "author": "Elena Shen",
  "license": "ISC",
  "devDependencies": {
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "@types/node": "^18.11.10",
    "@types/yargs": "^17.0.15",
    "chokidar": "^3.5.3",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "open": "^8.4.0",
    "yargs": "^17.6.2"
  }
}
